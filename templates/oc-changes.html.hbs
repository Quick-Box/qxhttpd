{{#*inline "page"}}

    <h1>OCheckList changes</h1>
    <h2><a href="/event/{{ event_id }}">{{ event_name }}</a></h2>

    {{#each change_set}}
        <p>Created <b>{{ this.Created }}</b></p>
        <table>
            <thead>
            <tr>
                <th>Id</th>
                <th>Status</th>
                <th>SI</th>
                <th>Class</th>
                <th>Name</th>
                <th>Start</th>
                <th>Comment</th>
            </tr>
            </thead>
            <tbody>
            {{#each this.Data}}
                <tr>
                    <td>{{ this.Runner.Id }}</td>
                    <td>{{ this.Runner.StartStatus }}</td>
                    <td>{{ this.Runner.Card }}</td>
                    <td>{{ this.Runner.ClassName }}</td>
                    <td>{{ this.Runner.Name }}</td>
                    <td>{{ this.Runner.StartTime }}</td>
                    <td>{{ this.Runner.Comment }}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    {{/each}}


{{/inline}}
{{> layout}}
