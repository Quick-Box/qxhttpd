{{#*inline "page"}}

    <h1>OCheckList changes</h1>
    <h2><a href="/event/{{ event.id }}">{{ event.name }}</a></h2>

    <table class="w3-table-all w3-hoverable">
        <thead>
        <tr class="w3-theme-l1">
            <th>Created</th>
            <th class="w3-right-align">Run Id</th>
            <th>Status</th>
            <th class="w3-right-align">SI</th>
            <th>Class</th>
            <th>Name</th>
            <th>Start</th>
            <th>Comment</th>
        </tr>
        </thead>
        <tbody>
        {{#each records}}
            <tr>
                <td>{{ this.0 }}</td>
            {{#with this.1.Runner}}
                <td class="w3-right-align">{{ Id }}</td>
                <td>{{ StartStatus }}</td>
                <td class="w3-right-align">{{ Card }}</td>
                <td>{{ ClassName }}</td>
                <td>{{ Name }}</td>
                <td>{{ StartTime }}</td>
                <td>{{ Comment }}</td>
            {{/with}}
            </tr>
        {{/each}}
        </tbody>
    </table>


{{/inline}}
{{> layout}}
