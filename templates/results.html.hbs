{{#*inline "page"}}

    <h2>Results</h2>
    <h3><a href="/event/{{ event.id }}">{{ event.name }}</a></h3>

    <div>
    {{#each classes}}
        <a class="w3-button" href="/event/{{ ../event.id }}/results?class_name={{this.name}}">{{ this.name }}</a>
    {{/each}}
    </div>

    <div class="w3-bar w3-theme-l2">
        <span class="w3-bar-item"><b>{{classrec.name}}</b></span>
        <span class="w3-bar-item w3-right">controls: {{classrec.control_count}}</span>
        <span class="w3-bar-item w3-right">climb: {{classrec.climb}}</span>
        <span class="w3-bar-item w3-right">length: {{classrec.length}}</span>
    </div>

    <table id="table" data-start00="{{ start00 }}" class="w3-table-all w3-hoverable">
        <thead>
        <tr class="w3-theme-l1">
            <th data-col-name="start_time" class="w3-right-align">Start</th>
            <th data-col-name="name">Name</th>
            <th data-col-name="registration">Registration</th>
            <th data-col-name="check_time" class="w3-right-align">Check</th>
            <th data-col-name="finish_time" class="w3-right-align">Finish</th>
            <th data-col-name="time" class="w3-right-align">Time</th>
            <th data-col-name="status">Status</th>
            <th data-col-name="run_id">Run Id</th>
        </tr>
        </thead>
        <tbody>
        {{#each runs}}
            <tr
                    data-start_time="{{ start_time }}"
                    data-finish_time="{{ finish_time }}"
                    data-last_name="{{ last_name }}"
                    data-first_name="{{ first_name }}"
                    data-registration="{{ registration }}"
                    data-si_id="{{ si_id }}"
                    data-run_id="{{ run_id }}"
                    data-status="{{ status }}"
            >
                <td class="w3-right-align"></td>
                <td></td>
                <td></td>
                <td class="w3-right-align"></td>
                <td class="w3-right-align"></td>
                <td class="w3-right-align"></td>
                <td></td>
                <td class="w3-right-align"></td>
            </tr>
        {{/each}}
        </tbody>
    </table>

    <script src="/js/utils.js"></script>
    <script>
    formatRunTable();
    </script>

{{/inline}}
{{> layout}}
